<div class="d-inline-flex bg-light">会員一覧</div>

  <table class="table table-hover table-border-bottom">
    <thead class ="thead-light">
       <tr>
         <th>会員ID</th>
         <th>氏名</th>
         <th>メールアドレス</th>
         <th>ステータス</th>
       <tr>
    </thead>

    <tbody>
     <% @customer.each do |customer| %>
      <tr>
        <td><%= customer.id %></td>
        <td><%= link_to admin_customer_path(customer.id) do %>
        <%= customer.first_name %><%= customer.last_name %>
        <% end %></td>

        <td><%= customer.email %></td>
        <td><%#= customer.is_deleted%></td>
      </tr>
      <% end %>
    </tbody>

